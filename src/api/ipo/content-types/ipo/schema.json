{
  "kind": "collectionType",
  "collectionName": "ipos",
  "info": {
    "singularName": "ipo",
    "pluralName": "ipos",
    "displayName": "IPO"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "uuid": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "CompanyName",
      "required": true
    },
    "CompanyName": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "price": {
      "type": "integer",
      "min": 0
    },
    "lotQty": {
      "type": "integer",
      "min": 0
    },
    "applicationAmount": {
      "type": "integer",
      "min": 0
    },
    "gmp": {
      "type": "integer",
      "min": 0
    },
    "retailQuota": {
      "type": "integer",
      "min": 0,
      "max": 100
    },
    "registrar": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::registrar.registrar",
      "inversedBy": "ipos"
    },
    "dates": {
      "type": "component",
      "component": "custom.dates",
      "repeatable": false
    }
  }
}
